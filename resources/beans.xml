<?xml version="1.0" encoding="UTF-8"?>
<!--suppress SpringFacetInspection -->
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- enable the usage of annotations -->
    <context:annotation-config/>

    <!--  -->
    <bean id="mov" class="sml.instruction.MovInstruction"/>
    <bean id="add" class="sml.instruction.AddInstruction"/>
    <bean id="sub" class="sml.instruction.SubInstruction"/>
    <bean id="div" class="sml.instruction.DivInstruction"/>
    <bean id="mul" class="sml.instruction.MulInstruction"/>
    <bean id="out" class="sml.instruction.OutInstruction"/>
    <bean id="jnz" class="sml.instruction.JnzInstruction"/>
    <bean id="registerFactory" class="sml.RegisterFactoryImpl"/>
    <bean id="intFactory" class="sml.intFactoryImpl"/>

</beans>